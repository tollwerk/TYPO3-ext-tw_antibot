{
  "name": "tollwerk/tw-antibot",
  "type": "typo3-cms-extension",
  "description": "Death to Spambots! â€” CAPTCHA-less spam security for TYPO3 forms",
  "homepage": "https://github.com/tollwerk/TYPO3-ext-tw_antibot",
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@tollwerk.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "typo3/cms-core": "^10.0",
    "jkphl/antibot": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Tollwerk\\TwAntibot\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tollwerk\\TwAntibot\\Tests\\": "Tests"
    }
  },
  "replace": {
    "tollwerk/tw-antibot": "self.version",
    "typo3-ter/tw-antibot": "self.version"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "vcs",
      "url": "git@github.com:jkphl/antibot.git",
      "reference": "master"
    }
  ],
  "extra": {
    "typo3/cms": {
      "extension-key": "tw_antibot"
    }
  }
}
